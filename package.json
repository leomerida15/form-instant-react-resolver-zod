    "dts-cli": "^2.0.5",
    "husky": "^9.1.5",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "size-limit": "^11.1.4",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  }
