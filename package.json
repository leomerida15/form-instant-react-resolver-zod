  "files": [
    "dist",
    "src"
  ],
  "private": false,
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests",
    "Up": "npm publish --access public"
